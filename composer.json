{
    "name": "bluex/twitter",
    "description": "Twitter Interface For laravel.",
    "keywords": ["Twitter", "laravel"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bluex\\Twitter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mohamed Eid",
            "email": "medoeid50@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Bluex\\Twitter\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bluex\\Twitter\\Providers\\TwitterServiceProvider"
            ]
        }
    },
    "scripts": {
    }
    
}
